define key as Integer attribute
define Entity as category with attributes key and name

define main as method doing:
    e1 = Entity with 3 as key and "Peter" as name
    e2 = Entity with 2 as key and "Alan" as name
    e3 = Entity with 1 as key and "John" as name
    list = [ e1, e2, e3 ]
    printLine "sorted -> " + sorted list