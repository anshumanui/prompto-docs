define main as method doing:
    t = "This is a text"
    c = 'h'
    f = "is"
    a = "hb"
    s = <'h', 'b'>
    print "c in t -> " + (c in t)
    print "f not in t -> " + (f not in t)
    print "a in t -> " + (a in t)
    print "t has f -> " + (t has f)
    print "t has any a -> " + (t has any a)
    print "t has all s -> " + (t has all s)